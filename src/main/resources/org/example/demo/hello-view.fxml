<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="186.0" prefWidth="239.0" spacing="20.0" xmlns="http://javafx.com/javafx/19-ea+7" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Button fx:id="btnEN" onAction="#onbtnENClick" text="EN" />
    <Button fx:id="btnFR" onAction="#onbtnFRClick" text="FR" />
    <Button fx:id="btnJP" onAction="#onbtnJPClick" text="JP" />
    <Button fx:id="btnIR" onAction="#onbtnIRClick" text="IR" />

    <Label fx:id="lbl" text="  ? ">
        <font>
            <Font size="18.0" />
        </font>
    </Label>

    <Label fx:id="welcomeText" text="Welcome! Please select a language." />
</VBox>
